{
  "name": "guestcms/git-commit-checker",
  "description": "Check coding standard & code syntax with Git pre-commit hook.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "guestcms",
    "guestcms cms",
    "guestcms platform",
    "guestcms git commit checker"
  ],
  "homepage": "https://coastalmediabrand.com",
  "authors": [
    {
      "name": "Justin Malonson",
      "email": "justin@coastalmediabrand.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Guestcms\\GitCommitChecker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Guestcms\\GitCommitChecker\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8.0|^8.1|^8.2|^8.3",
    "laravel/framework": ">=9.32",
    "laravel/pint": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0|^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Guestcms\\GitCommitChecker\\Providers\\GitCommitCheckerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
